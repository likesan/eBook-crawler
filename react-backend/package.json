{
  "name": "react-backend",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "nodemon node ./bin/www",
    "server": "nodemon node --inspect-brk ./bin/www",
    "client": "cd client && npm run start",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "heroku-postbuild": "npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "chrome-aws-lambda": "^2.0.2",
    "concurrently": "^5.2.0",
    "consolidate": "^0.15.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.17.3",
    "firebase-admin": "^8.9.0",
    "firebase-functions": "^3.3.0",
    "http-errors": "~1.6.3",
    "iltorb": "^2.4.4",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "mustache": "^3.2.1",
    "nodemon": "^2.0.2",
    "puppeteer": "^2.0.0",
    "puppeteer-cluster": "^0.18.0",
    "puppeteer-extra": "^3.1.7",
    "puppeteer-extra-plugin-adblocker": "^2.11.1",
    "puppeteer-extra-plugin-stealth": "^2.4.5",
    "react-gtm-module": "^2.0.8",
    "react-helmet": "^5.2.1",
    "react-scripts": "^3.4.1"
  }
}
